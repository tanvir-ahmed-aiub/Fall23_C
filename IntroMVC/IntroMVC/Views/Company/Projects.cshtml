
@{
    ViewBag.Title = "Projects";
}
<br />
<a class="btn btn-primary" href="/">Home</a>
<a class="btn btn-success" href="/company/projects">Projects</a>
<a class="btn btn-danger" href="/company/teams">Teams</a>
<h2>Projects</h2>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Cleint</th>
    </tr>
    @foreach (var p in Model) { 
        <tr>
            <td>@p.Name</td>
            <td>@p.Client</td>
        </tr>
    }
</table>
